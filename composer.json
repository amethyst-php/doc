{
    "name": "amethyst/doc",
    "keywords": ["laravel", "amethyst", "amethyst-documentation-generator", "documentation-generator", "documentation", "doc", "generator"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "rcrowe/twigbridge": "^0.9.6",
        "railken/bag": "^1.2",
        "hanneskod/classtools": "~1.0",
        "eloquent/composer-config-reader": "^2.1",
        "michelf/php-markdown": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.6.*",
        "amethyst/foo": "0.1.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\Documentation\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Documentation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Documentation\\GeneratorServiceProvider"
            ]
        }
    }
}
