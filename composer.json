{
    "name": "railken/amethyst-doc-generator",
    "keywords": ["laravel", "amethyst", "amethyst-documentation-generator", "documentation-generator", "documentation", "doc", "generator"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "rcrowe/twigbridge": "^0.9.6",
        "railken/bag": "^1.2",
        "railken/amethyst-common": "0.1.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.6.*",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/",
            "Railken\\Amethyst\\Tests\\App\\": "tests/App/src" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\DocGeneratorServiceProvider"
            ]
        }
    }
}
